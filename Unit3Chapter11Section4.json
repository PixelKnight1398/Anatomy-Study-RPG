          "group_name": "Microscopic Anatomy",
          "questions": [
            {
              "question": "Reticular fibers that surround individual muscle fibers, separating them from one another",
              "answers": [
                "endomysium"
              ]
            },
            {
              "question": "dense irregular connective tissue that surrounds each fascicle - fascicle: bundle of 10-100 muscle fibers (muscle cells)",
              "answers": [
                "perimysium"
              ]
            },
            {
              "question": "Outer layer encircling the entire muscle made of dense irregular connective tissue",
              "answers": [
                "epimysium"
              ]
            },
            {
              "question": "Plasma membrane of a muscle cell",
              "answers": [
                "sarcolemma"
              ]
            },
            {
              "question": "cytoplasm of a muscle fiber -> houses lots of glycogen (stored glucose) and mitochondria (used to make ATP)",
              "answers": [
                "sarcoplasm"
              ]
            },
            {
              "question": "Functional unit of a myofibril",
              "answers": [
                "sarcomere"
              ]
            }
          ]
